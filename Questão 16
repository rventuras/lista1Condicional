#include <stdio.h>
int main (){
    int ddd;
    printf ("Insira aqui o DDD: ");
    scanf ("%d", &ddd);
    switch (ddd)
    {
    case 61:
    printf ("Esse ddd eh de brasilia");
        break;
        case 71:
        printf ("Esse ddd eh de Salvador");
        break;
        case 11: 
        printf ("Esse ddd eh de SÃ£o Paulo");
        break;
        case 21:
        printf ("Esse ddd eh de Rio de Janeiro");
        break;
        case 32:
        printf ("Esse ddd eh de Juiz de Fora");
        break;
        case 19:
        printf ("Esse ddd eh de Campinas");
        break;
        case 27:
        printf ("Esse ddd eh de Vitoria");
        break;
        case 31: 
        printf ("Esse ddd eh de Belo Horizonte");
        break;

    
    default:
    printf ("ddd invalido");
        break;
    }
}
