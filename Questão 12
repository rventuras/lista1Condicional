#include <stdio.h>
int main (){
    int y,x;
    printf("Insira o valor de X: ");
    scanf("%d",&x);
    printf("Insira o valor de Y: ");
    scanf("%d",&y);
    if (x>0 && y>0)
    {
        printf("Q1");
    }else if(x<0 && y>0)
    {
        printf("Q2");
    }
    else if (x<0 && y<0)
    {
        printf("Q3");
    }else if(x>0 && y<0)
    {
        printf("Q4");
    }else if (x == 0 && y ==0)
    {
        printf("Origem");
    }else if (x == 0)
    {
        printf("Eixo X");
    }else if (y==0)
    {
        printf("Eixo Y");
    }
    }
